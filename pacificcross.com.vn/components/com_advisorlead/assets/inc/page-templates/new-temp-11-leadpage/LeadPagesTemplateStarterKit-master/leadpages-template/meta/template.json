            },
            "html": "<strong>Enter your email address below to reserve _______... it's 100% FREE!</strong>"
        },
        "submit": {
            "css": {
                "background-color": "#ffcb01;",
                "border-color": "#ffb403;",
                "border-width": "0pt;",
                "border-style": "solid;",
                "font-family": "Helvetica, Arial, sans-serif",
                "font-size": "14pt;",
                "letter-spacing": "-0.1em;"
